@font-face {
    font-family: 'League Gothic';
    src: url(<%= asset_path 'reveal.js/lib/font/league-gothic/league-gothic.eot' %>);
    src: url(<%= asset_path 'reveal.js/lib/font/league-gothic/league-gothic.eot?#iefix' %>) format('embedded-opentype'),
         url(<%= asset_path 'reveal.js/lib/font/league-gothic/league-gothic.woff' %>) format('woff'),
         url(<%= asset_path 'reveal.js/lib/font/league-gothic/league-gothic.ttf' %>) format('truetype');

    font-weight: normal;
    font-style: normal;
}